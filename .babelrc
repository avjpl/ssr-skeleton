{
  "presets": [
    "es2015",
    "react",
    [
      "env",
      {
        "modules": false,
        "useBuiltIns": true,
        "targets": {
          "browsers": ["last 2 Chrome versions"]
        }
      }
    ]
  ],
  "plugins": [
    "syntax-dynamic-import",
    "transform-class-properties",
    "transform-react-jsx-img-import",
    "transform-object-rest-spread"
  ]
}
